t});\n\n\t}\n\n\treturn (\n\t\t<React.Fragment>\n\t\t\t<NavComp\n\t\t\t\tkategoris={kategoris}\n\t\t\t\tgantiKategori={gantiKategori}\n\t\t\t\tkategoriAktif={kategoriAktif}\n\t\t\t/>\n\t\t\t<div className=\"container-menu\">\n\t\t\t\t<DaftarMenu kategoriAktif={kategoriAktif} tambahKeranjang={tambahKeranjang}/>\n\t\t\t\t<Keranjang\n\t\t\t\t\tkeranjangPost={keranjangPost}\n\t\t\t\t\ttambahPesanan={tambahPesanan}\n\t\t\t\t\tkurangPesanan={kurangPesanan}\n\t\t\t\t\thapusKeranjang={hapusKeranjang}\n\t\t\t\t/>\n\t\t\t</div>\n\t\t\t<TotalKeranjang\n\t\t\t\ttotalBelanja={totalBelanja}\n\t\t\t\tbuatPesanan={buatPesanan}\n\t\t\t/>\n\t\t</React.Fragment>\n\t)\n};\n\nexport default App;","/mnt/Data/CODING/react/besok/kasir-app/src/reportWebVitals.js",[],["117","118"],"/mnt/Data/CODING/react/besok/kasir-app/src/NavComp.js",[],"/mnt/Data/CODING/react/besok/kasir-app/src/DaftarMenu.js",[],"/mnt/Data/CODING/react/besok/kasir-app/src/CardDaftarMenu.js",[],["119","120"],"/mnt/Data/CODING/react/besok/kasir-app/src/Keranjang.js",[],"/mnt/Data/CODING/react/besok/kasir-app/src/ModalKeranjang.js",["121"],"import React from \"react\";\nimport { Modal } from \"react-bootstrap\";\n\nconst ModalKeranjang = ({handleClose, showModal, keranjangDetail}) => {\n\n\t// console.log(showModal)\n\n\treturn(\n\t\t<button onClick={() => handleClose()}>tekan</button>\n\t)\n\t\n};\n\nexport default ModalKeranjang;\n","/mnt/Data/CODING/react/besok/kasir-app/src/TotalKeranjang.js",[],{"ruleId":"122","replacedBy":"123"},{"ruleId":"124","replacedBy":"125"},{"ruleId":"126","severity":1,"message":"127","line":27,"column":5,"nodeType":"128","endLine":27,"endColumn":7,"suggestions":"129"},{"ruleId":"130","severity":1,"message":"131","line":2,"column":38,"nodeType":"132","messageId":"133","endLine":2,"endColumn":44},{"ruleId":"122","replacedBy":"134"},{"ruleId":"124","replacedBy":"135"},{"ruleId":"126","severity":1,"message":"136","line":31,"column":5,"nodeType":"128","endLine":31,"endColumn":7,"suggestions":"137"}